{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview-apk": {
      "android": { "buildType": "apk" }
    },
    "apk": {
      "android": { "buildType": "apk" }
    },
    "development": {
      "android": { "buildType": "app-bundle" },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": { "NODE_ENV": "production" }
    }
  },
  "submit": {
    "production": {}
  }
}
